//Printing of an array
#include <iostream>
using namespace std;

int main() {
    string a[4];
     cout<<"Print values of an array"<<endl;
    for (int i=0;i<4;i++){
       
        cin>>a[i];
    }
    cout<<"number at inde x 3 is "<<a[3]<<endl;
    cout<<"Print values of an array"<<" ";
    for (int i=3;i>=0;i--){
       
        cout<<a[i]<<" ";
    }
    

    return 0;
}
