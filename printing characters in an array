#include <iostream>
using namespace std;

int main() {
    char a[6];
     cout<<"Print values of an array"<<endl;
    for (int i=0;i<=6;i++){
       
        cin>>a[i];
    }
    cout<<"number at inde x 3 is "<<a[3]<<endl;
    cout<<"Print values of an array"<<" ";
    for (int i=6;i>=0;i--){
       
        cout<<a[i]<<" ";
    }
    

    return 0;
}
