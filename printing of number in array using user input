// Printing of an array
#include <iostream>
using namespace std;

int main() {
    int a[6];
     cout<<"Print values of an array"<<endl;
    for (int i=0;i<=6;i++){
       
        cin>>a[i];
    }
    cout<<"Print values of an array"<<" ";
    for (int i=0;i<=6;i++){
       
        cout<<a[i]<<" ";
    }
    

    return 0;
}
